+++ b/core/src/test/java/org/neo4j/ogm/metadata/GeneratedTests.java
@@ -0,0 +1,19 @@
+package org.neo4j.ogm.metadata;
+
+import org.junit.Test;
+
+public class GeneratedTests {
+
+    @Test
+    public void testRelationshipFieldsPerformance() {
+        ClassInfo classInfo = new ClassInfo(LotsOfRelationships.class, org.neo4j.ogm.driver.TypeSystem.NoNativeTypes.INSTANCE);
+        // call relationshipFields many times to simulate repeated mapping
+        long start = System.nanoTime();
+        for (int i = 0; i < 1000; i++) {
+            classInfo.relationshipFields();
+        }
+        long end = System.nanoTime();
+        System.out.println("Time elapsed (ms): " + ((end - start) / 1_000_000.0));
+    }
+}

diff --git a/EXPLANATION.md b/EXPLANATION.md
new file mode 100644
